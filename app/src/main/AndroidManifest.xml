<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="koshik.travelove">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="koshik.SwipeTravel.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="koshik.SwipeTravel.signinActivity"
            android:label="@string/title_activity_signin">

        </activity>
        <activity
            android:name="koshik.SwipeTravel.SignupActivity"
            android:label="@string/title_activity_signup">

        </activity>
        <activity
            android:name="koshik.SwipeTravel.MainHomePage"
            android:label="@string/title_activity_main2"></activity>
        <activity
            android:name="koshik.SwipeTravel.SwapTab"
            android:label="@string/title_activity_swap_tab"></activity>
    </application>

</manifest>
